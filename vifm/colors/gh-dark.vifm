highlight Executable  cterm=none  ctermfg=218  ctermbg=233    guifg=#fcb1ab     guibg=#0d1117
highlight Link        cterm=none  ctermfg=218  ctermbg=233    guifg=#fcb1ab     guibg=#0d1117
highlight BrokenLink  cterm=none  ctermfg=231  ctermbg=54     guifg=#ffffff     guibg=#963cf6
highlight Device      cterm=none  ctermfg=178  ctermbg=233    guifg=#faa356     guibg=#0d1117
highlight Fifo        cterm=none  ctermfg=153  ctermbg=233    guifg=#a2d2fb     guibg=#0d1117
highlight Socket      cterm=none  ctermfg=153  ctermbg=233    guifg=#a2d2fb     guibg=#0d1117

highlight {LICENSE}     cterm=none      ctermfg=243     ctermbg=233    guifg=#89929b     guibg=#0d1117
highlight {*.txt,*.md}  cterm=none      ctermfg=228     ctermbg=233    guifg=#fbef83     guibg=#0d1117
highlight {*.sh,*.bash} cterm=none      ctermfg=75      ctermbg=233    guifg=#77bdfb     guibg=#0d1117
highlight {*.go,*.py}   cterm=none      ctermfg=75      ctermbg=233    guifg=#77bdfb     guibg=#0d1117
highlight {*.vim,*.zsh} cterm=none      ctermfg=75      ctermbg=233    guifg=#77bdfb     guibg=#0d1117
highlight {*.tex}       cterm=none      ctermfg=27      ctermbg=233    guifg=#0887f7     guibg=#0d1117
highlight {*.css,*.lua} cterm=none      ctermfg=27      ctermbg=233    guifg=#0887f7     guibg=#0d1117
highlight {*README*}    cterm=none      ctermfg=0       ctermbg=178    guifg=#000000     guibg=#faa356
highlight {.*,*.conf}   cterm=none      ctermfg=183     ctermbg=233    guifg=#cea5fb     guibg=#0d1117
