highlight {*.sty}       cterm=none      ctermfg=178     ctermbg=233    guifg=#faa356     guibg=#0d1117
highlight {*.zip,*.rar,*.tar,*.gz,*.bz2,*.iso} cterm=underline ctermfg=54 ctermbg=233    guifg=#963cf6     guibg=#0d1117
highlight {*.ini,*.yml,*.json,*.desktop} cterm=none      ctermfg=183     ctermbg=233    guifg=#cea5fb     guibg=#0d1117
highlight {*.mov,*.mp4,*.gif,*.mpeg,*.mpg,*.flv,*.avi,*.png,*.bmp,*.jpg,*.jpeg,*.pnm,*.svg,*.ico} cterm=none ctermfg=178 ctermbg=233    guifg=#faa356     guibg=#0d1117
highlight {*.c,*.h,*.cxx,*.hxx,*.cpp,*.hpp,*.cu,*.cuh,*.cu.h,*.cs}   cterm=none      ctermfg=75      ctermbg=233    guifg=#77bdfb     guibg=#0d1117
highlight {*.pdf,*.doc,*.docx,*.xls,*.xlsx,*.ppt,*.pptx,*.csv} cterm=none      ctermfg=186     ctermbg=233    guifg=#d7d787     guibg=#0d1117
